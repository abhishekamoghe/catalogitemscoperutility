        <name>Copy to new App Scope Modal</name>
        <onclick>moveToNewScope()</onclick>
        <order>100</order>
        <script><![CDATA[/* global */
function moveToNewScope() {
	var dialogClass = (typeof GlideModal !== 'undefined') ? GlideModal : GlideDialogWindow;
	// eslint-disable-next-line new-cap
	var md = new dialogClass('sc_cat_item_convert_to_new_scope');
	
	//Sets the dialog title
	//md.setTitle(new GwtMessage().getMessage('Select new App Scope "{0}" to new App Scope', g_form.getValue('name')));
	md.setTitle(new GwtMessage().getMessage('Select new Application Scope'));
	//returns the value of the title
	
	md.setPreference('sysparm_total_qty', g_form.getValue('quantity'));
	md.setPreference('sysparm_sys_id', g_form.getUniqueValue());
	md.setPreference('sysparm_status', '6');
	md.setPreference('sysparm_substatus', 'pending_disposal');
	assetSetDomainParameters(md);
	md.setWidth(400);
	md.render();
}]]></script>
